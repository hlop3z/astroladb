{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "types/**/*.d.ts",
    "schemas/**/*.js",
    "migrations/**/*.js",
    "generators/**/*.js"
  ],
  "exclude": [
    "node_modules",
    ".alab"
  ]
}
